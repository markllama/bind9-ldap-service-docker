;;
;; An example zone file to show the proper elements of a 
;;
$TTL 3600
$ORIGIN example.com.
@ 1D    IN  SOA ns1.example.com. admin.example.com. (
  2017051702 ; serial
  3H ; refresh
  15 ; retry
  1w ; expire
  3h ; nxdomain ttl
)
  IN  NS  ns1.example.com.
  IN  NS  ns2.example.com.
  IN  MX  10 mail.example.com.
ns1   IN  A  192.168.1.2
ns2   IN  A  4.2.2.1
mail  IN  A  192.168.1.10
